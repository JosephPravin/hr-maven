pipeline {
	agent any 
	
	node{
		stage('Compile stage') {
			steps {
				withMaven(maven: 'maven_3_6_0') {
					sh 'mvn -f cisco-hr/pom.xml clean package'
				}
			}
		}
		
		stage('Test stage') {
			steps {
				echo "Running test scripts"
			}
		}
		
        	stage('Print working directory') {
			

                sshagent(['jpravinp']) {
                    sh 'pwd;'
                }
		
		}
	}
}

