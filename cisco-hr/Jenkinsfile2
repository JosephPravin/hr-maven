pipeline {
	agent any
	environment {
		ipAddress = '10.98.12.223'
	}
	stages {
		stage('Explode jar') {
			steps {
				sshagent(['jpravinp']) {
					sh 'ssh -tt -o StrictHostKeyChecking=no jpravinp@${ipAddress}'
			
					// jar xf /Users/jpravinp/Desktop/tomcat/webapps/cisco-hr-0.0.1-SNAPSHOT.jar
				}
			}
		}
	}
}
